<div class='error'>
    <% if flash[:my_errors]%>
        <% flash[:my_errors].each do |error|%>
            <p><%= error %></p>
        <%end%>
    <%end%>
</div>

<%= form_for @artist do |f|%>
    
    <%= f.label :name%>
    <%= f.text_field :name%>
    
    <%= f.label :title%>
    <%= f.text_field :title%>
    
    <%= f.label :age%>
    <%= f.text_field :age%>

  

    <%= f.submit "Create Artist"%>
<% end%>